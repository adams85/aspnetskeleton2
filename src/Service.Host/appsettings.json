{
  "AllowedHosts": "*",

  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "File": {
      "LogLevel": {
        "Default": "Warning"
      },
      "BasePath": "Logs",
      "EnsureBasePath": true,
      "Files": [
        {
          "Path": "<date:yyyyMMdd>/app.log"
        }
      ]
    }
  }
}
