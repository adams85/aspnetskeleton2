@page
@model AccessDeniedModel

@section Header {
    <h3 class="mb-0">@Model.Layout.Title</h3>
}

<p class="lead mb-0">
    <strong>@T["You do not have access to this resource."]</strong>
</p>

@section Footer {
    <div class="text-center">
        <a asp-page="@Areas.Dashboard.Pages.IndexModel.PageDescriptor.PageName" asp-area="@Areas.Dashboard.Pages.IndexModel.PageDescriptor.AreaName">@T["Back to Dashboard"]</a>
    </div>
}
