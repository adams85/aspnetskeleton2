@model WebApp.UI.Areas.Dashboard.Models.Home.HomeIndexModel

<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">@T["Overview"]</h3>
    </div>
    <div class="panel-body">
        <p>
            <span class="glyphicon glyphicon-time"></span>
            @T["{0} hour to midnight", Plural.From("{0} hours to midnight", (int)Model.TimeToMidnight.TotalHours), TextContext.From("Dashboard/Home")]
        </p>
    </div>
</div>
