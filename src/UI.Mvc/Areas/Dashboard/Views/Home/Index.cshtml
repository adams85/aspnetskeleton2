  <div class="panel-heading">
    <h3 class="panel-title">@T["Overview"]</h3>
  </div>
  <div class="panel-body">
      <p>@T["{0} hour to midnight", Plural.From("{0} hours to midnight", (int)Model.TimeToMidnight.TotalHours), TextContext.From("Dashboard/Home")]</p>
  </div>
</div>
